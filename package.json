{
    "name": "fleece",
    "displayName": "Fleece",
    "description": "Llama for VSCode",
    "version": "0.0.1",
    "publisher": "Kelden",
    "repository": {
        "type": "git",
        "url": "https://github.com/keldenl/fleece"
    },
    "engines": {
        "vscode": "^1.0.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "*",
        "onCommand:fleece.commentToCode",
        "onCommand:fleece.startDalai",
        "onCommand:fleece.stopFleece"
    ],
    "main": "./src/index.js",
    "contributes": {
        "commands": [{
                "command": "fleece.commentToCode",
                "title": "Convert Comment to Code",
                "category": "Fleece"
            },
            {
                "command": "fleece.autocomplete",
                "title": "Autocomplete",
                "category": "Fleece"
            },
            {
                "command": "fleece.startDalai",
                "title": "Start/Restart Dalai Server",
                "category": "Fleece"
            },
            {
                "command": "fleece.stopFleece",
                "title": "Stop generation",
                "category": "Fleece"
            }
        ],
        "menus": {
            "editor/context": [{
                "command": "fleece.commentToCode",
                "group": "edit"
            }]
        },
        "keybindings": [{
            "command": "fleece.commentToCode",
            "key": "ctrl+alt+c",
            "mac": "cmd+alt+c",
            "when": "editorTextFocus && !editorReadonly"
        }]
    },
    "scripts": {
        "build": "vsce package"
    },
    "devDependencies": {
        "@types/vscode": "^1.0.0",
        "typescript": "^5.0.2",
        "vscode-test": "^1.6.1",
        "@vscode/vsce": "^2.18.0"
    },
    "dependencies": {
        "socket.io-client": "^4.6.1"
    }
}